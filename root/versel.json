{
  "version": 2,
  "builds": [{
    "src": "backend/wsgi.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "15mb",
      "runtime": "python3.9",
      "buildCommand": "pip install --upgrade pip && pip install --no-cache-dir -r requirements.txt"
    }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "backend/wsgi.py"
    }
  ]
}