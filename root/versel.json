{
    "version": 2,
    "builds": [{
      "src": "backend/backend/wsgi.py",
      "use": "@vercel/python"
    }],
    "routes": [
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      },
      {
          "src": "/(.*)",
          "dest": "backend/backend/wsgi.py"
      }
    ]
  }